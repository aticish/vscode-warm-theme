                "markup.raw.block.markdown",
                "markup.inline.raw.string.markdown"
            ],
            "settings": {
                "foreground": "#527c96"
            }
        },

        // Other
        {
            "scope": [
                "comment.block.c",
                "punctuation.terminator.statement.c"
            ],
            "settings": {
                "foreground": "#575757"
            }
        },
        {
            "scope": [
                "comment.line.number-sign.ini",
                "comment.line.number-sign.yaml",
                "comment.line.number-sign.ignore"
            ],
            "settings": {
                "foreground": "#575757"
            }
        },
        {
            "scope": [
                "support.type.property-name.table.toml"
            ],
            "settings": {
                "foreground": "#3c997d"
            }
        },
        {
            "scope": [
                "keyword.operator",
                "keyword.operator.comparison",
                "keyword.operator.logical",
                "keyword.operator.key"
            ],
            "settings": {
                "foreground": "#8a8a8a"
            }
        },
        {
            "scope": [
                "keyword.other.definition.ini"
            ],
            "settings": {
                "foreground": "#a6704d"
            }
        },

        // Font Styles
        {
            "scope": [
                "emphasis",
                "markup.italic"
            ],
            "settings": {
                "fontStyle": "italic"
            }
        },
        {
            "scope": [
                "strong",
                "markup.bold",
                "markup.heading"
            ],
            "settings": {
                "fontStyle": "bold"
            }
        },
        {
            "scope": [
                "markup.underline",
                "markup.underline.link"
            ],
            "settings": {
                "fontStyle": "underline"
            }
        },
        {
            "scope": [
                "strikethrough",
                "markup.strikethrough",
                "markup.deleted"
            ],
            "settings": {
                "fontStyle": "strikethrough"
            }
        },
        {
            "scope": [
                "markup.italic markup.strikethrough"
            ],
            "settings": {
                "fontStyle": "italic strikethrough"
            }
        },
        {
            "scope": [
                "strong markup.strikethrough",
                "markup.bold markup.strikethrough",
                "markup.heading markup.strikethrough"
            ],
            "settings": {
                "fontStyle": "bold strikethrough"
            }
        },
        {
            "scope": [
                "strong markup.italic",
                "markup.bold markup.italic",
                "markup.heading markup.italic"
            ],
            "settings": {
                "fontStyle": "bold italic"
            }
        },
        {
            "scope": [
                "markup.heading strong",
                "markup.bold strong"
            ],
            "settings": {
                "fontStyle": "bold"
            }
        },
        {
            "scope": [
                "strong markup.italic markup.strikethrough",
                "markup.bold markup.italic markup.strikethrough",
                "markup.heading markup.italic markup.strikethrough"
            ],
            "settings": {
                "fontStyle": "bold strikethrough italic"
            }
        }

    ]
}

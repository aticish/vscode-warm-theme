            ],
            "settings": {
                "foreground": "#6C71C4"
            }
        },
        {
            "scope": [
                "punctuation.definition.quote.begin.markdown",
                "punctuation.definition.quote.end.markdown"
            ],
            "settings": {
                "foreground": "#6C71C4"
            }
        },
        {
            "scope": [
                "markup.raw.block.markdown",
                "markup.inline.raw.string.markdown"
            ],
            "settings": {
                "foreground": "#527c96"
            }
        },
        {
            "scope": [
                "markup.underline.link.markdown"
            ],
            "settings": {
                "foreground": "#4f772d"
            }
        },

        // Other
        {
            "scope": [
                "comment.block.c",
                "punctuation.terminator.statement.c"
            ],
            "settings": {
                "foreground": "#8da0a5"
            }
        },
        {
            "scope": [
                "comment.line.number-sign.ini",
                "comment.line.number-sign.yaml",
                "comment.line.number-sign.ignore"
            ],
            "settings": {
                "foreground": "#8da0a5"
            }
        },
        {
            "scope": [
                "keyword.other.definition.ini"
            ],
            "settings": {
                "foreground": "#254a53"
            }
        },
        {
            "scope": [
                "support.type.property-name.table.toml"
            ],
            "settings": {
                "foreground": "#7b2cbf"
            }
        },
        {
            "scope": [
                "keyword.operator.comparison",
                "keyword.operator.logical",
                "keyword.operator.key"
            ],
            "settings": {
                "foreground": "#1a404a"
            }
        },

        // Font Styles
        {
            "scope": [
                "emphasis",
                "markup.italic"
            ],
            "settings": {
                "fontStyle": "italic"
            }
        },
        {
            "scope": [
                "strong",
                "markup.bold",
                "markup.heading"
            ],
            "settings": {
                "fontStyle": "bold"
            }
        },
        {
            "scope": [
                "markup.underline",
                "markup.underline.link"
            ],
            "settings": {
                "fontStyle": "underline"
            }
        },
        {
            "scope": [
                "strikethrough",
                "markup.strikethrough",
                "markup.deleted"
            ],
            "settings": {
                "fontStyle": "strikethrough"
            }
        },
        {
            "scope": [
                "markup.italic markup.strikethrough"
            ],
            "settings": {
                "fontStyle": "italic strikethrough"
            }
        },
        {
            "scope": [
                "strong markup.strikethrough",
                "markup.bold markup.strikethrough",
                "markup.heading markup.strikethrough"
            ],
            "settings": {
                "fontStyle": "bold strikethrough"
            }
        },
        {
            "scope": [
                "strong markup.italic",
                "markup.bold markup.italic",
                "markup.heading markup.italic"
            ],
            "settings": {
                "fontStyle": "bold italic"
            }
        },
        {
            "scope": [
                "markup.heading strong",
                "markup.bold strong"
            ],
            "settings": {
                "fontStyle": "bold"
            }
        },
        {
            "scope": [
                "strong markup.italic markup.strikethrough",
                "markup.bold markup.italic markup.strikethrough",
                "markup.heading markup.italic markup.strikethrough"
            ],
            "settings": {
                "fontStyle": "bold strikethrough italic"
            }
        }

    ]
}
